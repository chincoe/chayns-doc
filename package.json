{
  "name": "chayns-doc",
  "version": "1.1.12",
  "description": "A documentation for the global chayns object for chayns development",
  "main": "src/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/index.d.ts",
    "dist/index.js"
  ],
  "scripts": {
    "pub": "npm publish",
    "pub:patch": "npm version patch --no-git-tag-version && npm publish",
    "pub:minor": "npm version minor --no-git-tag-version && npm publish",
    "pub:major": "npm version major --no-git-tag-version && npm publish",
    "preversion": "node deprecate.js",
    "postpublish": "node commit.js",
    "prepublishOnly": "copy \"src/\" \"dist/\" /y"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chincoe/chayns-doc.git"
  },
  "keywords": [
    "chayns"
  ],
  "author": "Christian Nagel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chincoe/chayns-doc/issues"
  },
  "homepage": "https://github.com/chincoe/chayns-doc#readme",
  "devDependencies": {
    "eslint": "^7.14.0",
    "eslint-plugin-jsdoc": "^30.7.8",
    "jsdoc": "^3.6.6",
    "tsd-jsdoc": "^2.5.0"
  }
}
